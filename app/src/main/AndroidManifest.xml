<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.nbdev.popularlibs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".mockito.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".mockito.view.MockitoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".di.dagger.DaggerActivity">
        </activity>
        <activity android:name=".di.withDi.DiActivity"></activity>
        <activity android:name=".di.withoutDi.NoDiActivity"></activity>
        <activity android:name=".room.RoomActivity"></activity>
        <activity android:name=".mvpnomoxy.NoMoxyActivity" />
        <activity android:name=".mvpmoxy.MoxyActivity" />
        <activity android:name=".AsyncTaskActivity" />
        <activity android:name=".observer.ObserverActivity" />
        <activity android:name=".rx_java.RxJavaActivity" />
        <activity android:name=".RxJavaButterKnifeActivity" />
        <activity android:name=".RxJavaButterKnifeSingleActivity" />
        <activity android:name=".gson.GsonActivity" />
        <activity android:name=".retrofit.RetrofitActivity" />
    </application>

</manifest>